local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Farming The Wall", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddToggle({
Name = "Start Autofarm",
Default = false,
Callback = function(Value)

_G.Autofarm = Value
while _G.Autofarm do
loadstring(game:HttpGet("https://raw.githubusercontent.com/IAmOnTablet/Speeding-Wall/main/The_Script"))()
task.wait()
end
    end
})
